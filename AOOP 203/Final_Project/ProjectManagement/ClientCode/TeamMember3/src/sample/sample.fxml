<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
    <columnConstraints>
        <ColumnConstraints />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints />
    </rowConstraints>
    <children>
        <AnchorPane prefHeight="740.0" prefWidth="802.0">
            <children>
                <Label layoutX="14.0" layoutY="14.0" prefHeight="28.0" prefWidth="187.0" text="Project Name : ">
                    <font>
                        <Font size="29.0" />
                    </font>
                </Label>
                <Label layoutX="210.0" layoutY="14.0" prefHeight="35.0" prefWidth="581.0" text="Project Management Software (Team Member)" textFill="#eb0e0eb0">
                    <font>
                        <Font size="29.0" />
                    </font>
                </Label>
                <Label layoutX="20.0" layoutY="58.0" prefHeight="27.0" prefWidth="136.0" text="Notice Board :">
                    <font>
                        <Font name="Chalkboard" size="20.0" />
                    </font>
                </Label>
                <TextArea fx:id="noticeTextArea" editable="false" layoutX="20.0" layoutY="85.0" prefHeight="113.0" prefWidth="771.0" promptText="Notice Board Area" />
                <TextArea fx:id="allTextArea" editable="false" layoutX="492.0" layoutY="258.0" prefHeight="394.0" prefWidth="299.0" promptText="Messenger Area" />
                <Label layoutX="562.0" layoutY="223.0" prefHeight="27.0" prefWidth="144.0" text="Messenger Box" textAlignment="CENTER">
                    <font>
                        <Font name="Chalkboard" size="20.0" />
                    </font>
                </Label>
                <TextField fx:id="msgTextField" layoutX="493.0" layoutY="663.0" prefHeight="55.0" prefWidth="218.0" />
                <Button layoutX="718.0" layoutY="671.0" mnemonicParsing="false" onAction="#msgSend" prefHeight="40.0" prefWidth="71.0" text="Send">
                    <font>
                        <Font name="Chalkboard" size="20.0" />
                    </font>
                </Button>
                <Label layoutX="20.0" layoutY="216.0" prefHeight="31.0" prefWidth="121.0" text="Task Board :">
                    <font>
                        <Font name="Chalkboard" size="20.0" />
                    </font>
                </Label>
                <Label layoutX="207.0" layoutY="452.0" prefHeight="26.0" prefWidth="86.0" text="Chat box" textAlignment="CENTER">
                    <font>
                        <Font name="Chalkboard" size="20.0" />
                    </font>
                </Label>
                <TextArea fx:id="assign3TextArea" editable="false" layoutX="16.0" layoutY="250.0" prefHeight="138.0" prefWidth="465.0" promptText="Task Area" />
                <TextArea fx:id="chat3TextArea" editable="false" layoutX="20.0" layoutY="486.0" prefHeight="144.0" prefWidth="459.0" promptText="Task Area" />
                <TextField fx:id="chat3TextField" layoutX="15.0" layoutY="652.0" prefHeight="38.0" prefWidth="365.0" />
                <Button layoutX="398.0" layoutY="654.0" minWidth="48.0" mnemonicParsing="false" onAction="#chat3" prefHeight="35.0" prefWidth="78.0" text="Send">
                    <font>
                        <Font name="Chalkboard" size="14.0" />
                    </font>
                </Button>
                <TextField fx:id="assign3TextField" layoutX="17.0" layoutY="404.0" prefHeight="38.0" prefWidth="345.0" />
                <Button layoutX="374.0" layoutY="406.0" minWidth="48.0" mnemonicParsing="false" onAction="#assign3" prefHeight="35.0" prefWidth="107.0" text="Update Task">
                    <font>
                        <Font name="Chalkboard" size="14.0" />
                    </font>
                </Button>
            </children>
        </AnchorPane>
    </children>
</GridPane>
